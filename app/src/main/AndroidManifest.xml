<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.androidl.uiwidget.sample.androidlnewuiwidgetsample" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MyActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".OldCardListActivity"
            android:label="@string/title_activity_old_card_list" >
        </activity>
        <activity
            android:name=".NewCardListActivity"
            android:label="@string/title_activity_new_card_list" >
        </activity>
    </application>

</manifest>
